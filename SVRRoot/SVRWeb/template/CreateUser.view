<% c++ #include "view/UserView.hpp" %>
<% skin SVRWebSkin %>
<% view CreateUser uses content::UserWithForm extends Main %>

<% template page_content() %>

    <% if not empty pageError %>
        <h3 style="color: blue"><%= pageError %></h3>
    <% else %>
        <form action="" method="post">
            <% form as_p form %>
        </form>
    <% end %>
<% end template %>

<% end view %>
<% end skin %>