SQL_PROPERTIES_DIR = ../SVRRoot/SVRPersist/postgres/
# To enable Unix socket set md5 authentication in /etc/postgresql/9.5/main/pg_hba.conf for local connections
# CONNECTION_STRING = dbname=svrwave user=svrwave password=svrwave host=/var/run/postgresql port=5432
CONNECTION_STRING = dbname=svrwave user=svrwave password=svrwave host=localhost
LOG_LEVEL = debug
DAO_TYPE = postgres
DONT_UPDATE_R_MATRIX = 1
MAX_SMO_ITERATIONS = 500000
CASCADE_MAX_SEGMENT_SIZE = 16500
CASCADE_BRANCHES_COUNT = 2
DISABLE_CASCADED_SVM = 0
MAIN_COLUMNS_AUX = 1
ENABLE_COMB_VALIDATE = 0
DEFAULT_COMB_VALIDATE_LIMIT = 20
# SVR_PARAMTUNE_COLUMN = close
SVR_PARAMTUNE_LEVEL = 10 11 12 13 14 15
MULTISTEP_LEN = 1

